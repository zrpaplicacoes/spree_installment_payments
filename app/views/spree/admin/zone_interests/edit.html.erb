<% content_for :page_title do %>
  <%= link_to Spree.t(:zone_interests), spree.admin_zone_interests_url %> /
  <%= @zone_interest.name %>
<% end %>

<%= form_for [:admin, @zone_interest] do |zone_interest_form| %>
  <%= render partial: 'form', locals: { zone_interest_form: zone_interest_form, zones: @zones, payment_methods: @payment_methods } %>

  <%= render partial: 'spree/admin/shared/edit_resource_links' %>
<% end %>
